<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.ww.mapper.TelNoteMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.ww.domain.TelNote">
        <id column="id" property="id" />
        <result column="tel_cdr_id" property="telCdrId" />
        <result column="phone" property="phone" />
        <result column="teacher_id" property="teacherId" />
        <result column="teacher_name" property="teacherName" />
        <result column="question_type" property="questionType" />
        <result column="question_level" property="questionLevel" />
        <result column="question" property="question" />
        <result column="answer" property="answer" />
        <result column="remark" property="remark" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="deleted" property="deleted" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, tel_cdr_id, phone, teacher_id, teacher_name, question_type, question_level, question, answer, remark, create_time, update_time, deleted
    </sql>

</mapper>
