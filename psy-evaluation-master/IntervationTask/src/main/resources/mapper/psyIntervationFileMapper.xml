<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.ww.mapper.PsyIntervationFileMapper">
    <select id="selectByStuId" parameterType="Long" resultType="org.ww.EntityDO.PsyIntervationFileDO">
        select *
        from psyintervationfiles
        where studentId=#{stuId}
    </select>

    <select id="selectLatestByStuId" parameterType="Long" resultType="org.ww.EntityDO.PsyIntervationFileDO">
        select *
        from psyintervationfiles
        where studentId=#{stuId}
        order by id desc limit 1;
    </select>
    <select id="selectByPage" parameterType="int" resultType="org.ww.EntityDO.PsyIntervationFileDO">
        select *
        from psyintervationfiles
        limit #{start},#{end};
    </select>



</mapper>